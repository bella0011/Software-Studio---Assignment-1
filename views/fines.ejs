<script>
    $(document).ready(function () {
        $('header').empty();
        $('footer').empty();
    });
</script>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" href="./src/app.css">
    <style>
        * {
            padding: 0;
            margin-left: 0;
            box-sizing: border-box;
            font-size: 1em;
        }

    /* NAVBAR BEGIN */
    nav {
        background-color: #011936;
        height: 82px;
        width: 100%;
    }

    label.logo {
        line-height: 80px;
        padding: 0px 80px;
    }

    nav ul {
        float: right;
        margin-right: 20px
    }

        nav ul li {
            display: inline-block;
            line-height: 80px;
            margin: 0 5px;
        }

            nav ul li a {
                font-size: 17px;
                color: white;
                text-transform: uppercase;
                padding: 7px 13px;
                border-radius: 3px;
            }

                a.active,
                nav ul li a:hover {
                    background: #49cee8;
                    transition: 0.5s;
                    text-decoration: none;
                    color: white;
                }

    .checkbtn {
        font-size: 30px;
        color: white;
        float: right;
        line-height: 80px;
        margin-right: 40px;
        cursor: pointer;
        display: none;
    }

    #check {
        display: none;
    }

    body {
        background: #F9DC5C;
    }

    .row {
        background: #f0f0f0;
        border-radius: 30px;
        box-shadow: 12px 12px 22px #465362;
    }

    img {
        border-radius: 15px;
    }

    div {
        margin: 25px
    }

    form {
        width: 400px;
        height: 600px;
    }

    @media (max-width: 952px) {
        label.logo {
            font-size: 30px;
            padding-left: 50px;
        }

        nav ul li a {
            font-size: 16px;
        }
    }

    @media (max-width: 858px) {
        .checkbtn {
            display: block;
        }

        ul {
            position: fixed;
            width: 100%;
            height: 100vh;
            background: #ED254E;
            top: 80px;
            left: -100%;
            text-align: center;
            transition: all .5s;
            z-index: 10;
        }

        nav ul li {
            display: block;
            margin: 50px 0;
            line-height: 30px;
        }

            nav ul li a {
                font-size: 20px;
            }

        a:hover,
        a.active {
            background-color: none;
            color: darkslateblue;
        }

        #check:checked ~ ul {
            left: 0;
        }
    }
</style>
</head>
    <body>
        <div>
            <h2> Fines </h2>
        </div>
        <div class="table-refresh" data-url="/data">  
                    
        </div>
        <script src="./src/app.js"></script>
    </body>
</html>